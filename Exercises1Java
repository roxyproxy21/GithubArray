public class Exercise1Java {
        public static int sumArray(int[] arr) {
            int sum = 0; 
            for (int value : arr) { 
                sum += value; 
            }
            return sum; 
        }
    
        public static void main(String[] args) {
            int[] arr = {25, 4, 16, 9, 10};
            int totalSum = sumArray(arr);
            System.out.println("The sum of the array's values is: " + totalSum);
        }
        
    }
